<p id="notice"><%= notice %></p>

<h1> レビューホーム</h1>
 <div class="row">
    <% @select_categories.each do |category| %>
       
          <div class="col-md-4 text-center">
            
          
              <td><%= image_tag(url_for(image_of_categories_path(category.category_id)), size: '100x100') %></td>
            
            
          </div>
        
    <% end %>
 </div>
<table>
  <thead>
    <tr>
      <%# <th>Category</th>
      <th>Goods review</th>
      <th>Store review</th>
      <th>Group</th> %>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @review_homes.each do |review_home| %>

   

    
      <tr>
        <td><%= review_home.category_id %></td>
        <td><%= review_home.goods_review_id %></td> 
        <td><%= review_home.store_review_id %></td> 
        <td><%= review_home.group_id %></td> 
        <td><%= link_to 'Show', review_home %></td>
        <td><%= link_to 'Edit', edit_review_home_path(review_home) %></td>
        <td><%= link_to 'Destroy', review_home, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

